{
  "name": "with-express",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "build": "cd ../frontend && yarn build && mkdir -p public && cd ../server && cp -R ../frontend/web-build/* public/",
    "dev": "nodemon server.ts",
    "serve": "ts-node server.ts",
    "start": "yarn build && yarn serve",
    "concurrent": "concurrently \"NODE_ENV=development nodemon server.ts\" \" cd searchkit-react && GRAPHQL_URL=http://localhost:4000/graphql PORT=3000 yarn dev\"",
    "tsc": "tsc server.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.10",
    "serve": "^12.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
